{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tauri dev",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "tauri-dev"],
